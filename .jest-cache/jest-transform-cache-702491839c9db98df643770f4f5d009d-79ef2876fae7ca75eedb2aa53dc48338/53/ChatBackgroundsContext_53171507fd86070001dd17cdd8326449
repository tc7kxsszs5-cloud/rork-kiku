import _defineProperty from "@babel/runtime/helpers/defineProperty";
import _asyncToGenerator from "@babel/runtime/helpers/asyncToGenerator";
import _slicedToArray from "@babel/runtime/helpers/slicedToArray";
import createContextHook from '@nkzw/create-context-hook';
import { useState, useCallback } from 'react';
import AsyncStorage from '@react-native-async-storage/async-storage';
import { logger } from '@/utils/logger';
var STORAGE_KEY = '@chat_backgrounds';
var DEFAULT_BACKGROUNDS = [{
  id: 'default',
  name: 'По умолчанию',
  type: 'color',
  value: '#FFFFFF'
}, {
  id: 'gradient1',
  name: 'Градиент 1',
  type: 'gradient',
  value: ['#4A90E2', '#357ABD']
}, {
  id: 'gradient2',
  name: 'Градиент 2',
  type: 'gradient',
  value: ['#FF6B35', '#E55A2B']
}];
var _createContextHook = createContextHook(function () {
    var _useState = useState(DEFAULT_BACKGROUNDS),
      _useState2 = _slicedToArray(_useState, 2),
      backgrounds = _useState2[0],
      setBackgrounds = _useState2[1];
    var _useState3 = useState({}),
      _useState4 = _slicedToArray(_useState3, 2),
      selectedBackgrounds = _useState4[0],
      setSelectedBackgrounds = _useState4[1];
    var setChatBackground = useCallback(function () {
      var _ref = _asyncToGenerator(function* (chatId, backgroundId) {
        setSelectedBackgrounds(function (prev) {
          return Object.assign({}, prev, _defineProperty({}, chatId, backgroundId));
        });
        try {
          yield AsyncStorage.setItem(STORAGE_KEY, JSON.stringify(Object.assign({}, selectedBackgrounds, _defineProperty({}, chatId, backgroundId))));
        } catch (error) {
          logger.error('Failed to save background', error instanceof Error ? error : new Error(String(error)), {
            context: 'ChatBackgroundsContext',
            action: 'setChatBackground',
            chatId: chatId,
            backgroundId: backgroundId
          });
        }
      });
      return function (_x, _x2) {
        return _ref.apply(this, arguments);
      };
    }(), [selectedBackgrounds]);
    var getChatBackground = useCallback(function (chatId) {
      var backgroundId = selectedBackgrounds[chatId];
      if (!backgroundId) {
        var defaultBg = backgrounds[0];
        if (!defaultBg) return null;
        if (defaultBg.type === 'color') {
          return Object.assign({}, defaultBg, {
            type: 'solid',
            color: defaultBg.value
          });
        }
        if (defaultBg.type === 'gradient') {
          return Object.assign({}, defaultBg, {
            gradient: defaultBg.value,
            gradientDirection: {
              start: {
                x: 0,
                y: 0
              },
              end: {
                x: 1,
                y: 1
              }
            }
          });
        }
        return defaultBg;
      }
      var bg = backgrounds.find(function (b) {
        return b.id === backgroundId;
      }) || backgrounds[0];
      if (!bg) return null;
      if (bg.type === 'color') {
        return Object.assign({}, bg, {
          type: 'solid',
          color: bg.value
        });
      }
      if (bg.type === 'gradient') {
        return Object.assign({}, bg, {
          gradient: bg.value,
          gradientDirection: {
            start: {
              x: 0,
              y: 0
            },
            end: {
              x: 1,
              y: 1
            }
          }
        });
      }
      return bg;
    }, [selectedBackgrounds, backgrounds]);
    return {
      backgrounds: backgrounds,
      selectedBackgrounds: selectedBackgrounds,
      setChatBackground: setChatBackground,
      getChatBackground: getChatBackground
    };
  }),
  _createContextHook2 = _slicedToArray(_createContextHook, 2),
  ChatBackgroundsProvider = _createContextHook2[0],
  useChatBackgrounds = _createContextHook2[1];
export { ChatBackgroundsProvider, useChatBackgrounds };
