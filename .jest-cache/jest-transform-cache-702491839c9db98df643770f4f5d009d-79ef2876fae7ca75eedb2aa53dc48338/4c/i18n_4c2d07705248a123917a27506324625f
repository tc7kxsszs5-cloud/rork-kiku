export var getLanguageByCode = function getLanguageByCode(code) {
  return availableLanguages.find(function (lang) {
    return lang.code === code;
  }) || availableLanguages[1];
};
export var getLanguagesByRegion = function getLanguagesByRegion(region) {
  return availableLanguages.filter(function (lang) {
    return lang.region === region || lang.region === 'Global';
  });
};
export var detectDeviceLanguage = function () {
  var _ref = _asyncToGenerator(function* () {
    try {
      var deviceLang = null;
      if (typeof navigator === 'undefined') {
        try {
          var _yield$import = yield import('expo-localization'),
            getLocales = _yield$import.getLocales;
          var locales = getLocales();
          if (locales && locales.length > 0) {
            var _locales$0$languageTa;
            deviceLang = locales[0].languageCode || ((_locales$0$languageTa = locales[0].languageTag) == null ? void 0 : _locales$0$languageTa.split('-')[0]) || null;
          }
        } catch (error) {
          console.warn('[i18n] expo-localization not available, using fallback');
        }
      } else {
        deviceLang = navigator.language.split('-')[0];
      }
      if (deviceLang) {
        var supported = availableLanguages.find(function (lang) {
          return lang.code === deviceLang;
        });
        if (!supported) {
          supported = availableLanguages.find(function (lang) {
            return lang.code.startsWith(deviceLang) || deviceLang.startsWith(lang.code.split('-')[0]);
          });
        }
        if (supported && !supported.comingSoon) {
          return supported.code;
        }
      }
    } catch (error) {
      console.error('[i18n] Error detecting device language:', error);
    }
    return 'en';
  });
  return function detectDeviceLanguage() {
    return _ref.apply(this, arguments);
  };
}();
