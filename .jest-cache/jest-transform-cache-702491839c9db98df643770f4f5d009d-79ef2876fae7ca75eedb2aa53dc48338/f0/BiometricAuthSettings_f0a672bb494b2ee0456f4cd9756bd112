import _asyncToGenerator from "@babel/runtime/helpers/asyncToGenerator";
import _slicedToArray from "@babel/runtime/helpers/slicedToArray";
import React, { useState } from 'react';
import { View, Text, StyleSheet, Switch, TouchableOpacity, Alert, ActivityIndicator, Platform } from 'react-native';
import { ScanFace, Fingerprint, Shield, ShieldOff } from 'lucide-react-native';
import { useSecuritySettings } from '@/constants/SecuritySettingsContext';
import { useThemeMode } from '@/constants/ThemeContext';
import { HapticFeedback } from '@/constants/haptics';
import { jsx as _jsx, jsxs as _jsxs, Fragment as _Fragment } from "react/jsx-runtime";
var BIOMETRIC_LABELS = {
  faceId: 'Face ID',
  touchId: 'Touch ID',
  fingerprint: 'Отпечаток пальца',
  none: 'Недоступно'
};
export function BiometricAuthSettings() {
  var _useThemeMode = useThemeMode(),
    theme = _useThemeMode.theme;
  var _useSecuritySettings = useSecuritySettings(),
    settings = _useSecuritySettings.settings,
    isLoading = _useSecuritySettings.isLoading,
    availableBiometrics = _useSecuritySettings.availableBiometrics,
    updateSettings = _useSecuritySettings.updateSettings,
    authenticate = _useSecuritySettings.authenticate;
  var _useState = useState(false),
    _useState2 = _slicedToArray(_useState, 2),
    isAuthenticating = _useState2[0],
    setIsAuthenticating = _useState2[1];
  var handleToggleBiometric = function () {
    var _ref = _asyncToGenerator(function* (value) {
      if (value && availableBiometrics === 'none') {
        Alert.alert('Биометрия недоступна', 'На вашем устройстве не настроена биометрическая аутентификация. Пожалуйста, настройте Face ID или Touch ID в настройках устройства.');
        return;
      }
      if (value) {
        setIsAuthenticating(true);
        try {
          var success = yield authenticate('Включить биометрическую аутентификацию');
          if (success) {
            HapticFeedback.success();
            yield updateSettings({
              biometricEnabled: true
            });
          } else {
            HapticFeedback.warning();
            Alert.alert('Ошибка', 'Не удалось подтвердить вашу личность');
          }
        } catch (error) {
          console.error('[BiometricAuthSettings] Authentication error:', error);
          HapticFeedback.error();
          Alert.alert('Ошибка', 'Не удалось включить биометрию');
        } finally {
          setIsAuthenticating(false);
        }
      } else {
        HapticFeedback.selection();
        yield updateSettings({
          biometricEnabled: false
        });
      }
    });
    return function handleToggleBiometric(_x) {
      return _ref.apply(this, arguments);
    };
  }();
  var handleTestAuth = function () {
    var _ref2 = _asyncToGenerator(function* () {
      setIsAuthenticating(true);
      HapticFeedback.medium();
      try {
        var success = yield authenticate('Тест биометрической аутентификации');
        if (success) {
          HapticFeedback.success();
          Alert.alert('Успешно', 'Биометрическая аутентификация работает корректно');
        } else {
          HapticFeedback.warning();
          Alert.alert('Ошибка', 'Аутентификация не прошла');
        }
      } catch (error) {
        console.error('[BiometricAuthSettings] Test auth error:', error);
        HapticFeedback.error();
        Alert.alert('Ошибка', 'Не удалось выполнить тест');
      } finally {
        setIsAuthenticating(false);
      }
    });
    return function handleTestAuth() {
      return _ref2.apply(this, arguments);
    };
  }();
  var styles = createStyles(theme);
  if (isLoading) {
    return _jsx(View, {
      style: styles.container,
      children: _jsx(ActivityIndicator, {
        size: "small",
        color: theme.accentPrimary
      })
    });
  }
  var biometricLabel = BIOMETRIC_LABELS[availableBiometrics];
  var isBiometricAvailable = availableBiometrics !== 'none';
  return _jsxs(View, {
    style: styles.container,
    children: [_jsxs(View, {
      style: styles.settingRow,
      children: [_jsxs(View, {
        style: styles.settingInfo,
        children: [_jsxs(View, {
          style: styles.settingHeader,
          children: [isBiometricAvailable ? _jsx(ScanFace, {
            size: 20,
            color: theme.accentPrimary
          }) : _jsx(ShieldOff, {
            size: 20,
            color: theme.textSecondary
          }), _jsx(Text, {
            style: styles.settingTitle,
            children: isBiometricAvailable ? biometricLabel : 'Биометрическая аутентификация'
          })]
        }), _jsx(Text, {
          style: styles.settingDescription,
          children: isBiometricAvailable ? `Использовать ${biometricLabel.toLowerCase()} для защиты приложения` : 'Биометрия недоступна на этом устройстве'
        }), !isBiometricAvailable && Platform.OS === 'ios' && _jsx(Text, {
          style: styles.settingHint,
          children: "\u041D\u0430\u0441\u0442\u0440\u043E\u0439\u0442\u0435 Face ID \u0438\u043B\u0438 Touch ID \u0432 \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0430\u0445 \u0443\u0441\u0442\u0440\u043E\u0439\u0441\u0442\u0432\u0430"
        })]
      }), _jsx(Switch, {
        value: settings.biometricEnabled,
        onValueChange: handleToggleBiometric,
        trackColor: {
          false: theme.borderSoft,
          true: theme.accentPrimary
        },
        thumbColor: theme.isDark ? '#fff' : '#fff',
        disabled: !isBiometricAvailable || isAuthenticating
      })]
    }), settings.biometricEnabled && _jsxs(View, {
      style: styles.infoCard,
      children: [_jsxs(View, {
        style: styles.infoHeader,
        children: [_jsx(Shield, {
          size: 18,
          color: theme.accentPrimary
        }), _jsx(Text, {
          style: styles.infoTitle,
          children: "\u0411\u0438\u043E\u043C\u0435\u0442\u0440\u0438\u044F \u0430\u043A\u0442\u0438\u0432\u043D\u0430"
        })]
      }), _jsxs(Text, {
        style: styles.infoText,
        children: ["\u041F\u0440\u0438\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0431\u0443\u0434\u0435\u0442 \u0437\u0430\u043F\u0440\u0430\u0448\u0438\u0432\u0430\u0442\u044C ", biometricLabel.toLowerCase(), " \u043F\u0440\u0438 \u043E\u0442\u043A\u0440\u044B\u0442\u0438\u0438 \u0438 \u043F\u0440\u0438 \u0434\u043E\u0441\u0442\u0443\u043F\u0435 \u043A \u0432\u0430\u0436\u043D\u044B\u043C \u0444\u0443\u043D\u043A\u0446\u0438\u044F\u043C."]
      })]
    }), settings.biometricEnabled && isBiometricAvailable && _jsx(TouchableOpacity, {
      style: [styles.testButton, isAuthenticating && styles.testButtonDisabled],
      onPress: handleTestAuth,
      disabled: isAuthenticating,
      children: isAuthenticating ? _jsx(ActivityIndicator, {
        size: "small",
        color: theme.accentPrimary
      }) : _jsxs(_Fragment, {
        children: [_jsx(Fingerprint, {
          size: 18,
          color: theme.accentPrimary
        }), _jsx(Text, {
          style: styles.testButtonText,
          children: "\u041F\u0440\u043E\u0442\u0435\u0441\u0442\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u0430\u0443\u0442\u0435\u043D\u0442\u0438\u0444\u0438\u043A\u0430\u0446\u0438\u044E"
        })]
      })
    }), !isBiometricAvailable && _jsxs(View, {
      style: styles.warningCard,
      children: [_jsxs(View, {
        style: styles.warningHeader,
        children: [_jsx(ShieldOff, {
          size: 18,
          color: "#f59e0b"
        }), _jsx(Text, {
          style: styles.warningTitle,
          children: "\u0411\u0438\u043E\u043C\u0435\u0442\u0440\u0438\u044F \u043D\u0435\u0434\u043E\u0441\u0442\u0443\u043F\u043D\u0430"
        })]
      }), _jsx(Text, {
        style: styles.warningText,
        children: "\u041D\u0430 \u0432\u0430\u0448\u0435\u043C \u0443\u0441\u0442\u0440\u043E\u0439\u0441\u0442\u0432\u0435 \u043D\u0435 \u043D\u0430\u0441\u0442\u0440\u043E\u0435\u043D\u0430 \u0431\u0438\u043E\u043C\u0435\u0442\u0440\u0438\u0447\u0435\u0441\u043A\u0430\u044F \u0430\u0443\u0442\u0435\u043D\u0442\u0438\u0444\u0438\u043A\u0430\u0446\u0438\u044F. \u041D\u0430\u0441\u0442\u0440\u043E\u0439\u0442\u0435 Face ID \u0438\u043B\u0438 Touch ID \u0432 \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0430\u0445 \u0443\u0441\u0442\u0440\u043E\u0439\u0441\u0442\u0432\u0430 \u0434\u043B\u044F \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u043D\u0438\u044F \u044D\u0442\u043E\u0439 \u0444\u0443\u043D\u043A\u0446\u0438\u0438."
      })]
    })]
  });
}
