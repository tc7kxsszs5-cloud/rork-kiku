import React from 'react';
import { render, fireEvent } from '@testing-library/react-native';
import { ThemeModeToggle } from '@/components/ThemeModeToggle';
import { jsx as _jsx } from "react/jsx-runtime";
describe('ThemeModeToggle', function () {
  beforeEach(function () {
    jest.clearAllMocks();
  });
  describe('Рендеринг', function () {
    it('должен отображать компонент переключения темы', function () {
      var _render = render(_jsx(ThemeModeToggle, {})),
        getByTestId = _render.getByTestId;
      expect(getByTestId('theme-toggle-button')).toBeTruthy();
    });
    it('должен использовать кастомный testID', function () {
      var _render2 = render(_jsx(ThemeModeToggle, {
          testID: "custom-toggle"
        })),
        getByTestId = _render2.getByTestId;
      expect(getByTestId('custom-toggle')).toBeTruthy();
    });
    it('должен показывать "День" в compact режиме для sunrise темы', function () {
      var _render3 = render(_jsx(ThemeModeToggle, {
          variant: "compact"
        })),
        getByText = _render3.getByText;
      expect(getByText('День')).toBeTruthy();
    });
    it('должен показывать "Светлый режим" в expanded режиме для sunrise темы', function () {
      var _render4 = render(_jsx(ThemeModeToggle, {
          variant: "expanded"
        })),
        getByText = _render4.getByText;
      expect(getByText('Светлый режим')).toBeTruthy();
    });
    it('должен показывать "Ночь" в compact режиме для midnight темы', function () {
      var _require = require('@/constants/ThemeContext'),
        useThemeMode = _require.useThemeMode;
      useThemeMode.mockReturnValue({
        themeMode: 'midnight',
        toggleThemeMode: jest.fn(),
        theme: {
          chipBackground: '#000000',
          chipText: '#ffffff',
          accentPrimary: '#4A90E2'
        }
      });
      var _render5 = render(_jsx(ThemeModeToggle, {
          variant: "compact"
        })),
        getByText = _render5.getByText;
      expect(getByText('Ночь')).toBeTruthy();
    });
    it('должен показывать "Ночной режим" в expanded режиме для midnight темы', function () {
      var _require2 = require('@/constants/ThemeContext'),
        useThemeMode = _require2.useThemeMode;
      useThemeMode.mockReturnValue({
        themeMode: 'midnight',
        toggleThemeMode: jest.fn(),
        theme: {
          chipBackground: '#000000',
          chipText: '#ffffff',
          accentPrimary: '#4A90E2'
        }
      });
      var _render6 = render(_jsx(ThemeModeToggle, {
          variant: "expanded"
        })),
        getByText = _render6.getByText;
      expect(getByText('Ночной режим')).toBeTruthy();
    });
  });
  describe('Переключение темы', function () {
    it('должен вызывать toggleThemeMode при нажатии', function () {
      var _require3 = require('@/constants/ThemeContext'),
        useThemeMode = _require3.useThemeMode;
      var mockToggle = jest.fn();
      useThemeMode.mockReturnValue({
        themeMode: 'sunrise',
        toggleThemeMode: mockToggle,
        theme: {
          chipBackground: '#ffffff',
          chipText: '#000000',
          accentPrimary: '#4A90E2'
        }
      });
      var _render7 = render(_jsx(ThemeModeToggle, {})),
        getByTestId = _render7.getByTestId;
      fireEvent.press(getByTestId('theme-toggle-button'));
      expect(mockToggle).toHaveBeenCalled();
    });
    it('должен вызывать HapticFeedback при переключении', function () {
      var _require4 = require('@/constants/haptics'),
        HapticFeedback = _require4.HapticFeedback;
      var _render8 = render(_jsx(ThemeModeToggle, {})),
        getByTestId = _render8.getByTestId;
      fireEvent.press(getByTestId('theme-toggle-button'));
      expect(HapticFeedback.selection).toHaveBeenCalled();
    });
  });
  describe('Варианты отображения', function () {
    it('должен применять compact стили для variant="compact"', function () {
      var _render9 = render(_jsx(ThemeModeToggle, {
          variant: "compact"
        })),
        getByTestId = _render9.getByTestId;
      var button = getByTestId('theme-toggle-button');
      expect(button.props.style).toContainEqual(expect.objectContaining({
        paddingHorizontal: 12,
        paddingVertical: 6
      }));
    });
    it('должен применять expanded стили для variant="expanded"', function () {
      var _render10 = render(_jsx(ThemeModeToggle, {
          variant: "expanded"
        })),
        getByTestId = _render10.getByTestId;
      var button = getByTestId('theme-toggle-button');
      expect(button.props.style).toContainEqual(expect.objectContaining({
        paddingHorizontal: 18,
        paddingVertical: 10
      }));
    });
    it('должен использовать compact по умолчанию', function () {
      var _render11 = render(_jsx(ThemeModeToggle, {})),
        getByTestId = _render11.getByTestId;
      var button = getByTestId('theme-toggle-button');
      expect(button.props.style).toContainEqual(expect.objectContaining({
        paddingHorizontal: 12,
        paddingVertical: 6
      }));
    });
  });
  describe('Стилизация', function () {
    it('должен применять переданный style', function () {
      var customStyle = {
        marginTop: 10
      };
      var _render12 = render(_jsx(ThemeModeToggle, {
          style: customStyle
        })),
        getByTestId = _render12.getByTestId;
      var button = getByTestId('theme-toggle-button');
      expect(button.props.style).toContainEqual(customStyle);
    });
    it('должен использовать цвета из темы', function () {
      var _render13 = render(_jsx(ThemeModeToggle, {})),
        getByTestId = _render13.getByTestId;
      var button = getByTestId('theme-toggle-button');
      expect(button.props.style).toContainEqual(expect.objectContaining({
        backgroundColor: '#ffffff',
        borderColor: '#4A90E2'
      }));
    });
  });
});
