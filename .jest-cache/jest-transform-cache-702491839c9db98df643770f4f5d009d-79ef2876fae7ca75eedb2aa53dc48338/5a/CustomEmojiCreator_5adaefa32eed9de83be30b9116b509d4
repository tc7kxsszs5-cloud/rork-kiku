import { View, Text, StyleSheet, TouchableOpacity, TextInput, ScrollView, Image, Alert, Modal } from 'react-native';
import { X, Upload, Save, Image as ImageIcon } from 'lucide-react-native';
import { requestMediaLibraryPermissionsAsync, launchImageLibraryAsync, MediaTypeOptions } from 'expo-image-picker';
import { useThemeMode } from '@/constants/ThemeContext';
import { addCustomEmoji } from '@/utils/customEmojis';
import { HapticFeedback } from '@/constants/haptics';
import { jsx as _jsx, jsxs as _jsxs } from "react/jsx-runtime";
export function CustomEmojiCreator(_ref) {
  var visible = _ref.visible,
    onClose = _ref.onClose,
    onEmojiCreated = _ref.onEmojiCreated;
  var _useThemeMode = useThemeMode(),
    theme = _useThemeMode.theme;
  var styles = useMemo(function () {
    return createStyles(theme);
  }, [theme]);
  var _useState = useState(''),
    _useState2 = _slicedToArray(_useState, 2),
    name = _useState2[0],
    setName = _useState2[1];
  var _useState3 = useState(''),
    _useState4 = _slicedToArray(_useState3, 2),
    category = _useState4[0],
    setCategory = _useState4[1];
  var _useState5 = useState(''),
    _useState6 = _slicedToArray(_useState5, 2),
    tags = _useState6[0],
    setTags = _useState6[1];
  var _useState7 = useState(null),
    _useState8 = _slicedToArray(_useState7, 2),
    imageUri = _useState8[0],
    setImageUri = _useState8[1];
  var _useState9 = useState('image'),
    _useState10 = _slicedToArray(_useState9, 2),
    emojiType = _useState10[0],
    setEmojiType = _useState10[1];
  var pickImage = function () {
    var _ref2 = _asyncToGenerator(function* () {
      try {
        var _yield$requestMediaLi = yield requestMediaLibraryPermissionsAsync(),
          status = _yield$requestMediaLi.status;
        if (status !== 'granted') {
          Alert.alert('Ошибка', 'Нужно разрешение на доступ к галерее');
          return;
        }
        var result = yield launchImageLibraryAsync({
          mediaTypes: MediaTypeOptions.Images,
          allowsEditing: true,
          aspect: [1, 1],
          quality: 0.8
        });
        if (!result.canceled && result.assets[0]) {
          setImageUri(result.assets[0].uri);
          HapticFeedback.light();
        }
      } catch (error) {
        console.error('[CustomEmojiCreator] Failed to pick image:', error);
        Alert.alert('Ошибка', 'Не удалось загрузить изображение');
      }
    });
    return function pickImage() {
      return _ref2.apply(this, arguments);
    };
  }();
  var handleCreate = function () {
    var _ref3 = _asyncToGenerator(function* () {
      if (!name.trim()) {
        Alert.alert('Ошибка', 'Введите название эмодзи');
        return;
      }
      if (emojiType === 'image' && !imageUri) {
        Alert.alert('Ошибка', 'Выберите изображение');
        return;
      }
      try {
        var newEmoji = {
          id: `custom_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
          name: name.trim(),
          type: emojiType,
          source: imageUri || '',
          category: category.trim() || undefined,
          tags: tags.split(',').map(function (t) {
            return t.trim();
          }).filter(function (t) {
            return t.length > 0;
          })
        };
        yield addCustomEmoji(newEmoji);
        HapticFeedback.success();
        Alert.alert('Успех', 'Кастомный эмодзи создан!');
        setName('');
        setCategory('');
        setTags('');
        setImageUri(null);
        onEmojiCreated == null || onEmojiCreated(newEmoji);
        onClose();
      } catch (error) {
        console.error('[CustomEmojiCreator] Failed to create emoji:', error);
        Alert.alert('Ошибка', 'Не удалось создать эмодзи');
      }
    });
    return function handleCreate() {
      return _ref3.apply(this, arguments);
    };
  }();
  return _jsx(Modal, {
    visible: visible,
    transparent: true,
    animationType: "slide",
    onRequestClose: onClose,
    children: _jsx(View, {
      style: styles.modalOverlay,
      children: _jsxs(View, {
        style: styles.modalContent,
        children: [_jsxs(View, {
          style: styles.header,
          children: [_jsx(Text, {
            style: styles.headerTitle,
            children: "\u0421\u043E\u0437\u0434\u0430\u0442\u044C \u043A\u0430\u0441\u0442\u043E\u043C\u043D\u044B\u0439 \u044D\u043C\u043E\u0434\u0437\u0438"
          }), _jsx(TouchableOpacity, {
            onPress: onClose,
            style: styles.closeButton,
            children: _jsx(X, {
              size: 20,
              color: theme.textPrimary
            })
          })]
        }), _jsxs(ScrollView, {
          style: styles.content,
          showsVerticalScrollIndicator: false,
          children: [imageUri && _jsx(View, {
            style: styles.previewContainer,
            children: _jsx(Image, {
              source: {
                uri: imageUri
              },
              style: styles.previewImage
            })
          }), _jsxs(View, {
            style: styles.inputGroup,
            children: [_jsx(Text, {
              style: styles.label,
              children: "\u041D\u0430\u0437\u0432\u0430\u043D\u0438\u0435 *"
            }), _jsx(TextInput, {
              style: styles.input,
              value: name,
              onChangeText: setName,
              placeholder: "\u041D\u0430\u043F\u0440\u0438\u043C\u0435\u0440: KIKU Logo",
              placeholderTextColor: theme.textSecondary
            })]
          }), _jsxs(View, {
            style: styles.inputGroup,
            children: [_jsx(Text, {
              style: styles.label,
              children: "\u041A\u0430\u0442\u0435\u0433\u043E\u0440\u0438\u044F"
            }), _jsx(TextInput, {
              style: styles.input,
              value: category,
              onChangeText: setCategory,
              placeholder: "\u041D\u0430\u043F\u0440\u0438\u043C\u0435\u0440: brand, safety, custom",
              placeholderTextColor: theme.textSecondary
            })]
          }), _jsxs(View, {
            style: styles.inputGroup,
            children: [_jsx(Text, {
              style: styles.label,
              children: "\u0422\u0435\u0433\u0438 (\u0447\u0435\u0440\u0435\u0437 \u0437\u0430\u043F\u044F\u0442\u0443\u044E)"
            }), _jsx(TextInput, {
              style: styles.input,
              value: tags,
              onChangeText: setTags,
              placeholder: "\u041D\u0430\u043F\u0440\u0438\u043C\u0435\u0440: logo, kiku, brand",
              placeholderTextColor: theme.textSecondary
            })]
          }), emojiType === 'image' && _jsxs(TouchableOpacity, {
            style: styles.imagePickerButton,
            onPress: pickImage,
            children: [_jsx(Upload, {
              size: 20,
              color: theme.accentPrimary
            }), _jsx(Text, {
              style: styles.imagePickerText,
              children: imageUri ? 'Изменить изображение' : 'Выбрать изображение'
            })]
          }), _jsxs(View, {
            style: styles.typeSelector,
            children: [_jsx(Text, {
              style: styles.label,
              children: "\u0422\u0438\u043F \u044D\u043C\u043E\u0434\u0437\u0438"
            }), _jsxs(View, {
              style: styles.typeButtons,
              children: [_jsxs(TouchableOpacity, {
                style: [styles.typeButton, emojiType === 'image' && styles.typeButtonActive],
                onPress: function onPress() {
                  return setEmojiType('image');
                },
                children: [_jsx(ImageIcon, {
                  size: 18,
                  color: emojiType === 'image' ? '#fff' : theme.textPrimary
                }), _jsx(Text, {
                  style: [styles.typeButtonText, emojiType === 'image' && styles.typeButtonTextActive],
                  children: "\u0418\u0437\u043E\u0431\u0440\u0430\u0436\u0435\u043D\u0438\u0435"
                })]
              }), _jsx(TouchableOpacity, {
                style: [styles.typeButton, emojiType === 'svg' && styles.typeButtonActive],
                onPress: function onPress() {
                  return setEmojiType('svg');
                },
                children: _jsx(Text, {
                  style: [styles.typeButtonText, emojiType === 'svg' && styles.typeButtonTextActive],
                  children: "SVG"
                })
              }), _jsx(TouchableOpacity, {
                style: [styles.typeButton, emojiType === 'unicode' && styles.typeButtonActive],
                onPress: function onPress() {
                  return setEmojiType('unicode');
                },
                children: _jsx(Text, {
                  style: [styles.typeButtonText, emojiType === 'unicode' && styles.typeButtonTextActive],
                  children: "Unicode"
                })
              })]
            })]
          }), _jsxs(TouchableOpacity, {
            style: [styles.createButton, !name.trim() && styles.createButtonDisabled],
            onPress: handleCreate,
            disabled: !name.trim(),
            children: [_jsx(Save, {
              size: 18,
              color: "#fff"
            }), _jsx(Text, {
              style: styles.createButtonText,
              children: "\u0421\u043E\u0437\u0434\u0430\u0442\u044C \u044D\u043C\u043E\u0434\u0437\u0438"
            })]
          })]
        })]
      })
    })
  });
