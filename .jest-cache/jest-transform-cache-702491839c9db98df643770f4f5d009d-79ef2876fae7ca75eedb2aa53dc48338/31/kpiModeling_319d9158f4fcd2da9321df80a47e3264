function calculateGrowthProjection() {
  var initialInstalls = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 1000;
  var growthRate = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : [1.5, 1.0, 0.6, 0.5, 0.25];
  var activationRate = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 0.6;
  var dauRate = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : 0.3;
  var retentionD7 = arguments.length > 4 && arguments[4] !== undefined ? arguments[4] : [0.35, 0.40, 0.42, 0.45, 0.45, 0.45];
  var retentionD30 = arguments.length > 5 && arguments[5] !== undefined ? arguments[5] : [0.20, 0.25, 0.28, 0.30, 0.30, 0.30];
  var premiumPrice = arguments.length > 6 && arguments[6] !== undefined ? arguments[6] : 9.99;
  var premiumConversionRates = arguments.length > 7 && arguments[7] !== undefined ? arguments[7] : [0, 0, 0, 0.03, 0.05, 0.06];
  var projections = [];
  var currentInstalls = initialInstalls;
  for (var month = 1; month <= 6; month++) {
    var activations = Math.round(currentInstalls * activationRate);
    var dau = Math.round(currentInstalls * dauRate);
    var mau = currentInstalls;
    var premiumConversionRate = premiumConversionRates[month - 1] || 0;
    var premiumUsers = Math.round(mau * premiumConversionRate);
    var mrr = premiumUsers * premiumPrice;
    var arr = mrr * 12;
    projections.push({
      month: month,
      installs: currentInstalls,
      activations: activations,
      dau: dau,
      mau: mau,
      retentionD7: retentionD7[month - 1] || 0.45,
      retentionD30: retentionD30[month - 1] || 0.30,
      premiumUsers: premiumUsers,
      mrr: mrr,
      arr: arr
    });
    if (month < 6) {
      var growth = growthRate[month - 1] || 1.25;
      currentInstalls = Math.round(currentInstalls * growth);
    }
  }
  return projections;
}
function calculateUnitEconomics() {
  var cac = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 4;
  var premiumPrice = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 9.99;
  var averageLifetimeMonths = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 12;
  var premiumConversionRate = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : 0.05;
  var freeUserValue = arguments.length > 4 && arguments[4] !== undefined ? arguments[4] : 15;
  var ltvPremium = premiumPrice * averageLifetimeMonths;
  var ltvFree = freeUserValue;
  var ltv = ltvPremium * premiumConversionRate + ltvFree * (1 - premiumConversionRate);
  var ltvCacRatio = ltv / cac;
  var averageRevenuePerUser = premiumPrice * premiumConversionRate;
  return {
    cac: cac,
    ltv: ltv,
    ltvCacRatio: ltvCacRatio,
    premiumConversionRate: premiumConversionRate,
    averageRevenuePerUser: averageRevenuePerUser
  };
}
function checkKPITargets(projections, unitEconomics) {
  var month6 = projections[5];
  var targets = {
    dau: 10000,
    mau: 50000,
    userGrowthRate: 25,
    retentionD7: 40,
    retentionD30: 25,
    activationRate: 50,
    premiumConversionRate: 5,
    cac: 10,
    ltv: 50,
    ltvCacRatio: 3
  };
  var achieved = {
    dau: month6.dau >= targets.dau,
    mau: month6.mau >= targets.mau,
    retentionD7: month6.retentionD7 * 100 >= targets.retentionD7,
    retentionD30: month6.retentionD30 * 100 >= targets.retentionD30,
    premiumConversionRate: unitEconomics.premiumConversionRate * 100 >= targets.premiumConversionRate,
    cac: unitEconomics.cac <= targets.cac,
    ltv: unitEconomics.ltv >= targets.ltv,
    ltvCacRatio: unitEconomics.ltvCacRatio >= targets.ltvCacRatio
  };
  if (projections.length >= 2) {
    var month5 = projections[4];
    var _month = projections[5];
    var growthRate = (_month.mau - month5.mau) / month5.mau * 100;
    achieved.userGrowthRate = growthRate >= targets.userGrowthRate;
  }
  var activationRate = month6.activations / month6.installs * 100;
  achieved.activationRate = activationRate >= targets.activationRate;
  return {
    targets: targets,
    achieved: achieved
  };
}
function generateInvestorProjection() {
  var projections = calculateGrowthProjection();
  var unitEconomics = calculateUnitEconomics();
  var kpiStatus = checkKPITargets(projections, unitEconomics);
  var month6 = projections[5];
  var totalUsers = month6.mau;
  var totalRevenue = projections.reduce(function (sum, p) {
    return sum + p.mrr;
  }, 0);
  var arr = month6.arr;
  var allTargetsMet = Object.values(kpiStatus.achieved).every(function (v) {
    return v === true;
  });
  return {
    projections: projections,
    unitEconomics: unitEconomics,
    kpiStatus: kpiStatus,
    summary: {
      totalUsers: totalUsers,
      totalRevenue: totalRevenue,
      arr: arr,
      allTargetsMet: allTargetsMet
    }
  };
}
