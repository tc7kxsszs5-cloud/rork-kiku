var getAndroidPriorityForRiskLevel = exports.getAndroidPriorityForRiskLevel = function getAndroidPriorityForRiskLevel(riskLevel) {
  switch (riskLevel) {
    case 'critical':
      return Notifications.AndroidNotificationPriority.MAX;
    case 'high':
