import _slicedToArray from "@babel/runtime/helpers/slicedToArray";
import React, { useState } from 'react';
import { View, Text, StyleSheet, TouchableOpacity, Image, Platform } from 'react-native';
import { X, Play } from 'lucide-react-native';
import { useThemeMode } from '@/constants/ThemeContext';
import { Video as ExpoVideo, ResizeMode } from 'expo-av';
import { jsx as _jsx, jsxs as _jsxs } from "react/jsx-runtime";
export function StatusViewer(_ref) {
  var statuses = _ref.statuses,
    currentIndex = _ref.currentIndex,
    onClose = _ref.onClose,
    onNext = _ref.onNext,
    onPrevious = _ref.onPrevious;
  var _useThemeMode = useThemeMode(),
    theme = _useThemeMode.theme;
  var _useState = useState(true),
    _useState2 = _slicedToArray(_useState, 2),
    isPlaying = _useState2[0],
    setIsPlaying = _useState2[1];
  var _useState3 = useState(null),
    _useState4 = _slicedToArray(_useState3, 2),
    videoRef = _useState4[0],
    setVideoRef = _useState4[1];
  var currentStatus = statuses[currentIndex];
  var styles = createStyles(theme);
  if (!currentStatus) {
    return null;
  }
  var handlePlayPause = function handlePlayPause() {
    if (currentStatus.type === 'video' && videoRef) {
      if (isPlaying) {
        videoRef.pauseAsync();
      } else {
        videoRef.playAsync();
      }
      setIsPlaying(!isPlaying);
    }
  };
  return _jsxs(View, {
    style: styles.container,
    children: [_jsxs(View, {
      style: styles.header,
      children: [_jsxs(View, {
        style: styles.userInfo,
        children: [currentStatus.userAvatar && _jsx(Image, {
          source: {
            uri: currentStatus.userAvatar
          },
          style: styles.avatar
        }), _jsxs(View, {
          children: [_jsx(Text, {
            style: styles.userName,
            children: currentStatus.userName
          }), _jsx(Text, {
            style: styles.timeAgo,
            children: new Date(currentStatus.createdAt).toLocaleTimeString('ru-RU', {
              hour: '2-digit',
              minute: '2-digit'
            })
          })]
        })]
      }), _jsx(TouchableOpacity, {
        onPress: onClose,
        style: styles.closeButton,
        children: _jsx(X, {
          size: 24,
          color: "#fff"
        })
      })]
    }), _jsxs(View, {
      style: styles.mediaContainer,
      children: [currentStatus.type === 'photo' ? _jsx(Image, {
        source: {
          uri: currentStatus.mediaUrl
        },
        style: styles.media,
        resizeMode: "contain"
      }) : _jsxs(View, {
        style: styles.videoContainer,
        children: [_jsx(ExpoVideo, {
          ref: function ref(_ref2) {
            return setVideoRef(_ref2);
          },
          source: {
            uri: currentStatus.mediaUrl
          },
          style: styles.video,
          shouldPlay: isPlaying,
          isLooping: true,
          resizeMode: ResizeMode.CONTAIN
        }), _jsx(TouchableOpacity, {
          onPress: handlePlayPause,
          style: styles.playButton,
          children: !isPlaying && _jsx(Play, {
            size: 48,
            color: "#fff"
          })
        })]
      }), currentStatus.caption && _jsx(View, {
        style: styles.captionContainer,
        children: _jsx(Text, {
          style: styles.caption,
          children: currentStatus.caption
        })
      })]
    }), _jsxs(View, {
      style: styles.navigation,
      children: [_jsx(TouchableOpacity, {
        onPress: onPrevious,
        style: styles.navButton,
        children: _jsx(Text, {
          style: styles.navButtonText,
          children: "\u2190"
        })
      }), _jsx(View, {
        style: styles.dots,
        children: statuses.map(function (_, index) {
          return _jsx(View, {
            style: [styles.dot, index === currentIndex && styles.dotActive]
          }, index);
        })
      }), _jsx(TouchableOpacity, {
        onPress: onNext,
        style: styles.navButton,
        children: _jsx(Text, {
          style: styles.navButtonText,
          children: "\u2192"
        })
      })]
    })]
  });
}
var createStyles = function createStyles(theme) {
  return StyleSheet.create({
    container: {
      flex: 1,
      backgroundColor: '#000'
    },
    header: {
      flexDirection: 'row',
      justifyContent: 'space-between',
      alignItems: 'center',
      padding: 16,
      paddingTop: Platform.OS === 'ios' ? 50 : 16
    },
    userInfo: {
      flexDirection: 'row',
      alignItems: 'center',
      gap: 12
    },
    avatar: {
      width: 40,
      height: 40,
      borderRadius: 20,
      borderWidth: 2,
      borderColor: '#fff'
    },
    userName: {
      color: '#fff',
      fontSize: 16,
      fontWeight: '600'
    },
    timeAgo: {
      color: '#999',
      fontSize: 12
    },
    closeButton: {
      padding: 8
    },
    mediaContainer: {
      flex: 1,
      justifyContent: 'center',
      alignItems: 'center'
    },
    media: {
      width: '100%',
      height: '100%'
    },
    videoContainer: {
      width: '100%',
      height: '100%',
      justifyContent: 'center',
      alignItems: 'center'
    },
    video: {
      width: '100%',
      height: '100%'
    },
    playButton: {
      position: 'absolute',
      justifyContent: 'center',
      alignItems: 'center'
    },
    captionContainer: {
      position: 'absolute',
      bottom: 100,
      left: 0,
      right: 0,
      padding: 16
    },
    caption: {
      color: '#fff',
      fontSize: 16,
      textShadowColor: 'rgba(0, 0, 0, 0.5)',
      textShadowOffset: {
        width: 0,
        height: 1
      },
      textShadowRadius: 3
    },
    navigation: {
      flexDirection: 'row',
      justifyContent: 'space-between',
      alignItems: 'center',
      padding: 16,
      paddingBottom: Platform.OS === 'ios' ? 40 : 16
    },
    navButton: {
      padding: 12
    },
    navButtonText: {
      color: '#fff',
      fontSize: 24,
      fontWeight: 'bold'
    },
    dots: {
      flexDirection: 'row',
      gap: 8
    },
    dot: {
      width: 8,
      height: 8,
      borderRadius: 4,
      backgroundColor: 'rgba(255, 255, 255, 0.3)'
    },
    dotActive: {
      backgroundColor: '#fff',
      width: 24
    }
  });
};
