import React from 'react';
import { View, Image, StyleSheet, TouchableOpacity, Text } from 'react-native';
import { SvgXml } from 'react-native-svg';
import { jsx as _jsx } from "react/jsx-runtime";
export function CustomEmoji(_ref) {
  var emoji = _ref.emoji,
    _ref$size = _ref.size,
    size = _ref$size === void 0 ? 32 : _ref$size,
    onPress = _ref.onPress;
  var renderEmoji = function renderEmoji() {
    switch (emoji.type) {
      case 'image':
        return _jsx(Image, {
          source: {
            uri: emoji.source
          },
          style: [styles.emojiImage, {
            width: size,
            height: size
          }],
          resizeMode: "contain"
        });
      case 'svg':
        return _jsx(View, {
          style: [styles.emojiContainer, {
            width: size,
            height: size
          }],
          children: _jsx(SvgXml, {
            xml: emoji.source,
            width: size,
            height: size
          })
        });
      case 'unicode':
        return _jsx(Text, {
          style: [styles.emojiText, {
            fontSize: size
          }],
          children: emoji.source
        });
      default:
        return null;
    }
  };
  if (onPress) {
    return _jsx(TouchableOpacity, {
      onPress: onPress,
      style: [styles.emojiWrapper, {
        width: size + 8,
        height: size + 8
      }],
      activeOpacity: 0.7,
      children: renderEmoji()
    });
  }
  return _jsx(View, {
    style: [styles.emojiWrapper, {
      width: size + 8,
      height: size + 8
    }],
    children: renderEmoji()
  });
}
var styles = StyleSheet.create({
  emojiWrapper: {
    justifyContent: 'center',
    alignItems: 'center',
    padding: 4
  },
  emojiContainer: {
    justifyContent: 'center',
    alignItems: 'center'
  },
  emojiImage: {
    borderRadius: 4
  },
  emojiText: {
    textAlign: 'center'
  }
});
