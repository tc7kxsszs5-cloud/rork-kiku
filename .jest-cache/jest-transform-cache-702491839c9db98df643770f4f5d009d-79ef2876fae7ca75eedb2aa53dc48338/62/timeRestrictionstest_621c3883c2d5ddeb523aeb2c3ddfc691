725c170baaf51a40e7d704b735233248
var _timeRestrictions = require("@/utils/timeRestrictions");
describe('isTimeRestricted', function () {
  it('должен возвращать false для пустого массива ограничений', function () {
    var restrictions = [];
    var now = new Date('2024-01-15T14:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(false);
  });
  it('должен возвращать false если ограничение отключено', function () {
    var restrictions = [{
      enabled: false,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T14:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(false);
  });
  it('должен возвращать false если день недели не совпадает', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 2,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T14:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(false);
  });
  it('должен возвращать true если время до начала разрешенного периода', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T08:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(true);
  });
  it('должен возвращать false если время в разрешенном периоде', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T14:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(false);
  });
  it('должен возвращать true если время после окончания разрешенного периода', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T18:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(true);
  });
  it('должен возвращать false если время равно началу периода (граничный случай)', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T09:00:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(false);
  });
  it('должен возвращать true если время равно концу периода (граничный случай)', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now = new Date('2024-01-15T17:00:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now)).toBe(true);
  });
  it('должен работать с несколькими ограничениями (возвращает true если хотя бы одно активно)', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 12,
      endMinute: 0
    }, {
      enabled: true,
      dayOfWeek: 1,
      startHour: 13,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var now1 = new Date('2024-01-15T11:00:00');
    var now2 = new Date('2024-01-15T12:30:00');
    var now3 = new Date('2024-01-15T15:00:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now1)).toBe(false);
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now2)).toBe(true);
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now3)).toBe(false);
  });
  it('должен работать с timestamp вместо Date объекта', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 9,
      startMinute: 0,
      endHour: 17,
      endMinute: 0
    }];
    var timestamp = new Date('2024-01-15T14:30:00').getTime();
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, timestamp)).toBe(false);
  });
  it('должен правильно обрабатывать полночь (переход через день - ограничение реализации)', function () {
    var restrictions = [{
      enabled: true,
      dayOfWeek: 1,
      startHour: 22,
      startMinute: 0,
      endHour: 2,
      endMinute: 0
    }];
    var now1 = new Date('2024-01-15T23:30:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now1)).toBe(true);
    var now2 = new Date('2024-01-16T01:30:00');
    var now3 = new Date('2024-01-16T03:00:00');
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now2)).toBe(false);
    expect((0, _timeRestrictions.isTimeRestricted)(restrictions, now3)).toBe(false);
  });
});
//# sourceMappingURL=data:application/json;charset=utf-8;base64,