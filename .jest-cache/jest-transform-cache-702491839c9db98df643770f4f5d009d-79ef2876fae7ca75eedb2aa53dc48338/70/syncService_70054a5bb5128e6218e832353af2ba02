import { logger } from '@/utils/logger';
function getDeviceId() {
  return _getDeviceId.apply(this, arguments);
}
function _getDeviceId() {
  _getDeviceId = _asyncToGenerator(function* () {
    var stored = yield AsyncStorage.getItem('@device_id');
    if (stored) {
      return stored;
    }
    var deviceId = `device_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`;
    yield AsyncStorage.setItem('@device_id', deviceId);
    return deviceId;
  });
  return _getDeviceId.apply(this, arguments);
}
function getLastSyncTimestamp(_x) {
  return _getLastSyncTimestamp.apply(this, arguments);
}
function _getLastSyncTimestamp() {
  _getLastSyncTimestamp = _asyncToGenerator(function* (key) {
    var stored = yield AsyncStorage.getItem(key);
    return stored ? parseInt(stored, 10) : null;
  });
  return _getLastSyncTimestamp.apply(this, arguments);
}
function setLastSyncTimestamp(_x2, _x3) {
  return _setLastSyncTimestamp.apply(this, arguments);
}
function _setLastSyncTimestamp() {
  _setLastSyncTimestamp = _asyncToGenerator(function* (key, timestamp) {
    yield AsyncStorage.setItem(key, timestamp.toString());
  });
  return _setLastSyncTimestamp.apply(this, arguments);
}
function isTimeoutError(error) {
  if (error instanceof Error) {
    return error.name === 'AbortError' || error.message.includes('timeout') || error.message.includes('Timeout') || error.message.includes('network') || error.message.includes('Network') || error.message.includes('fetch failed') || error.message.includes('Failed to fetch');
  }
  return false;
}
function isRetryableError(error) {
  if (error instanceof Error) {
    return isTimeoutError(error) || error.message.includes('ECONNREFUSED') || error.message.includes('ENOTFOUND') || error.message.includes('ETIMEDOUT') || error.message.includes('500') || error.message.includes('502') || error.message.includes('503') || error.message.includes('504');
  }
  return false;
}
function withRetry(_x4) {
  return _withRetry.apply(this, arguments);
}
function _withRetry() {
  _withRetry = _asyncToGenerator(function* (fn) {
    var maxRetries = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 3;
    var initialDelay = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 1000;
    var context = arguments.length > 3 ? arguments[3] : undefined;
    var lastError;
    var _loop = function* _loop() {
        try {
          return {
            v: yield fn()
          };
        } catch (error) {
          lastError = error;
          if (attempt === maxRetries - 1 || !isRetryableError(error)) {
            throw error;
          }
          var delay = initialDelay * Math.pow(2, attempt);
          logger.warn('Sync retry attempt', Object.assign({
            component: (context == null ? void 0 : context.component) || 'SyncService',
            action: (context == null ? void 0 : context.action) || 'sync',
            attempt: attempt + 1,
            maxRetries: maxRetries,
            delay: delay,
            error: error instanceof Error ? error.message : String(error)
          }, context));
          yield new Promise(function (resolve) {
            return setTimeout(resolve, delay);
          });
        }
      },
      _ret;
    for (var attempt = 0; attempt < maxRetries; attempt++) {
      _ret = yield* _loop();
      if (_ret) return _ret.v;
    }
    throw lastError || new Error('Max retries exceeded');
  });
  return _withRetry.apply(this, arguments);
}
var ChatSyncService = function () {
  function ChatSyncService() {
    _classCallCheck(this, ChatSyncService);
    this.lastSyncKey = '@chats_last_sync';
    this.initialized = false;
  }
  return _createClass(ChatSyncService, [{
    key: "initialize",
    value: function () {
      var _initialize = _asyncToGenerator(function* () {
        if (this.initialized) return;
        this.initialized = true;
      });
      function initialize() {
        return _initialize.apply(this, arguments);
      }
      return initialize;
    }()
  }, {
    key: "syncChats",
    value: function () {
      var _syncChats = _asyncToGenerator(function* (chats) {
        try {
          yield this.initialize();
          var deviceId = yield getDeviceId();
          var lastSync = yield getLastSyncTimestamp(this.lastSyncKey);
          var result = yield withRetry(function () {
            return trpcVanillaClient.sync.chats.sync.mutate({
              deviceId: deviceId,
              chats: chats,
              lastSyncTimestamp: lastSync || 0
            });
          }, 3, 1000, {
            component: 'ChatSyncService',
            action: 'syncChats',
            deviceId: deviceId,
            chatsCount: chats.length
          });
          if (result.success && result.chats) {
            yield setLastSyncTimestamp(this.lastSyncKey, Date.now());
            logger.info('Chats synced successfully', {
              component: 'ChatSyncService',
              action: 'syncChats',
              deviceId: deviceId,
              chatsCount: result.chats.length
            });
            return {
              success: true,
              data: result.chats
            };
          }
          return {
            success: false,
            error: 'Sync failed'
          };
        } catch (error) {
          var errorMessage = error instanceof Error ? error.message : 'Unknown error';
          var isTimeout = isTimeoutError(error);
          logger.error('Chat sync failed', error instanceof Error ? error : new Error(errorMessage), {
            component: 'ChatSyncService',
            action: 'syncChats',
            isTimeout: isTimeout,
            errorMessage: errorMessage
          });
          return {
            success: false,
            error: isTimeout ? 'Синхронизация прервана из-за таймаута. Проверьте подключение к интернету.' : errorMessage
          };
        }
      });
      function syncChats(_x5) {
        return _syncChats.apply(this, arguments);
      }
      return syncChats;
    }()
  }, {
    key: "getChats",
    value: function () {
      var _getChats = _asyncToGenerator(function* () {
        try {
          yield this.initialize();
          var deviceId = yield getDeviceId();
          var lastSync = yield getLastSyncTimestamp(this.lastSyncKey);
          var result = yield withRetry(function () {
            return trpcVanillaClient.sync.chats.get.query({
              deviceId: deviceId,
              lastSyncTimestamp: lastSync || 0
            });
          }, 3, 1000, {
            component: 'ChatSyncService',
            action: 'getChats',
            deviceId: deviceId
          });
          if (result && result.chats) {
            yield setLastSyncTimestamp(this.lastSyncKey, Date.now());
            logger.info('Chats retrieved successfully', {
              component: 'ChatSyncService',
              action: 'getChats',
              deviceId: deviceId,
              chatsCount: result.chats.length
            });
            return {
              success: true,
              data: result.chats
            };
          }
          return {
            success: false,
            error: 'Get failed'
          };
        } catch (error) {
          var errorMessage = error instanceof Error ? error.message : 'Unknown error';
          var isTimeout = isTimeoutError(error);
          logger.error('Get chats failed', error instanceof Error ? error : new Error(errorMessage), {
            component: 'ChatSyncService',
            action: 'getChats',
            isTimeout: isTimeout,
            errorMessage: errorMessage
          });
          return {
            success: false,
            error: isTimeout ? 'Не удалось загрузить чаты. Проверьте подключение к интернету.' : errorMessage
          };
        }
      });
      function getChats() {
        return _getChats.apply(this, arguments);
      }
      return getChats;
    }()
  }]);
}();
