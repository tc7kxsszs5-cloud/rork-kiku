var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _asyncToGenerator2 = _interopRequireDefault(require("@babel/runtime/helpers/asyncToGenerator"));
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _reactQuery = require("@tanstack/react-query");
var _jsxRuntime = require("react/jsx-runtime");
describe('Контексты', function () {
  var queryClient;
  beforeEach(function () {
    queryClient = new _reactQuery.QueryClient({
      defaultOptions: {
        queries: {
          retry: false
        },
        mutations: {
          retry: false
        }
      }
    });
  });
  var testContext = function testContext(contextName, hookName) {
    test(`${contextName} инициализируется без ошибок`, (0, _asyncToGenerator2.default)(function* () {
      try {
        var module = require(`@/constants/${contextName}`);
        var Provider = module[`${contextName.replace('Context', '')}Provider`] || module.Provider;
        var useHook = module[hookName] || module.useHook;
        if (!Provider || !useHook) {
          console.warn(`⚠️  ${contextName} не экспортирует Provider или hook`);
          return;
        }
        var TestComponent = function TestComponent() {
          var hook = useHook();
          return (0, _jsxRuntime.jsx)("div", {
            children: "Test"
          });
        };
        (0, _reactNative.render)((0, _jsxRuntime.jsx)(_reactQuery.QueryClientProvider, {
          client: queryClient,
          children: (0, _jsxRuntime.jsx)(Provider, {
            children: (0, _jsxRuntime.jsx)(TestComponent, {})
          })
        }));
        yield (0, _reactNative.waitFor)(function () {
          expect(true).toBe(true);
        });
      } catch (error) {
        throw new Error(`${contextName} не работает: ${error}`);
      }
    }));
  };
  testContext('ThemeContext', 'useThemeMode');
  testContext('AuthContext', 'useAuth');
  testContext('UserContext', 'useUser');
  testContext('MonitoringContext', 'useMonitoring');
  testContext('SyncSettingsContext', 'useSyncSettings');
  testContext('SecuritySettingsContext', 'useSecuritySettings');
});
