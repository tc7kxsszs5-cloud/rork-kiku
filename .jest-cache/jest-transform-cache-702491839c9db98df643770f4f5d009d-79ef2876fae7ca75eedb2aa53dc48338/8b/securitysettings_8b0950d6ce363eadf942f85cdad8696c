import _toConsumableArray from "@babel/runtime/helpers/toConsumableArray";
import React, { useMemo } from 'react';
import { ScrollView, View, Text, StyleSheet } from 'react-native';
import { LinearGradient } from 'expo-linear-gradient';
import { Shield, TrendingUp, AlertCircle, MessageCircle, RefreshCcw } from 'lucide-react-native';
import { useMonitoring } from '@/constants/MonitoringContext';
import { useUser } from '@/constants/UserContext';
import { useThemeMode } from '@/constants/ThemeContext';
import { jsx as _jsx, jsxs as _jsxs } from "react/jsx-runtime";
var RISK_COLORS = {
  safe: '#10b981',
  low: '#3b82f6',
  medium: '#f59e0b',
  high: '#ef4444',
  critical: '#991b1b'
};
var RISK_LABELS = {
  safe: 'Безопасно',
  low: 'Низкий',
  medium: 'Средний',
  high: 'Высокий',
  critical: 'Критический'
};
export default function SecuritySettingsScreen() {
  var _user$name, _user$children$length, _user$children;
  var _useMonitoring = useMonitoring(),
    chats = _useMonitoring.chats,
    alerts = _useMonitoring.alerts;
  var _useUser = useUser(),
    user = _useUser.user;
  var _useThemeMode = useThemeMode(),
    theme = _useThemeMode.theme;
  var styles = useMemo(function () {
    return createStyles(theme);
  }, [theme]);
  var statistics = useMemo(function () {
    var totalMessages = chats.reduce(function (sum, chat) {
      return sum + chat.messages.length;
    }, 0);
    var analyzedMessages = chats.reduce(function (sum, chat) {
      return sum + chat.messages.filter(function (message) {
        return message.analyzed;
      }).length;
    }, 0);
    var riskDistribution = {
      safe: 0,
      low: 0,
      medium: 0,
      high: 0,
      critical: 0
    };
    var chatRiskDistribution = {
      safe: 0,
      low: 0,
      medium: 0,
      high: 0,
      critical: 0
    };
    chats.forEach(function (chat) {
      chat.messages.forEach(function (message) {
        if (message.analyzed && message.riskLevel) {
          riskDistribution[message.riskLevel] += 1;
        }
      });
      chatRiskDistribution[chat.overallRisk] += 1;
    });
    var unresolvedCount = alerts.filter(function (alertItem) {
      return !alertItem.resolved;
    }).length;
    var resolvedCount = alerts.length - unresolvedCount;
    return {
      totalMessages: totalMessages,
      analyzedMessages: analyzedMessages,
      riskDistribution: riskDistribution,
      chatRiskDistribution: chatRiskDistribution,
      unresolvedCount: unresolvedCount,
      resolvedCount: resolvedCount,
      totalChats: chats.length
    };
  }, [alerts, chats]);
  var maxRiskCount = Math.max.apply(Math, _toConsumableArray(Object.values(statistics.riskDistribution)).concat([1]));
  return _jsxs(ScrollView, {
    style: styles.container,
    contentContainerStyle: styles.contentContainer,
    testID: "security-settings-screen",
    children: [_jsxs(LinearGradient, {
      colors: theme.heroGradient,
      style: styles.heroCard,
      children: [_jsxs(View, {
        style: styles.heroHeader,
        children: [_jsx(Shield, {
          size: 36,
          color: theme.isDark ? '#fef3c7' : '#0b1220'
        }), _jsxs(View, {
          style: styles.heroTextWrapper,
          children: [_jsx(Text, {
            style: styles.heroTitle,
            children: "\u0426\u0435\u043D\u0442\u0440 \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u0438"
          }), _jsxs(Text, {
            style: styles.heroSubtitle,
            children: [(_user$name = user == null ? void 0 : user.name) != null ? _user$name : 'Родитель', " \u2022 ", ((_user$children$length = user == null || (_user$children = user.children) == null ? void 0 : _user$children.length) != null ? _user$children$length : 0) > 0 || user != null && user.email ? 'Родитель' : 'Ребенок']
          })]
        })]
      }), _jsxs(View, {
        style: styles.heroBadges,
        children: [_jsxs(View, {
          style: styles.badge,
          children: [_jsx(MessageCircle, {
            size: 18,
            color: theme.isDark ? '#0b1220' : '#fef3c7'
          }), _jsxs(Text, {
            style: styles.badgeText,
            children: [statistics.totalChats, " \u0447\u0430\u0442\u043E\u0432 \u043F\u043E\u0434 \u0437\u0430\u0449\u0438\u0442\u043E\u0439"]
          })]
        }), _jsxs(View, {
          style: styles.badge,
          children: [_jsx(TrendingUp, {
            size: 18,
            color: theme.isDark ? '#0b1220' : '#fef3c7'
          }), _jsxs(Text, {
            style: styles.badgeText,
            children: [statistics.analyzedMessages, " \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439 \u043F\u0440\u043E\u0430\u043D\u0430\u043B\u0438\u0437\u0438\u0440\u043E\u0432\u0430\u043D\u043E"]
          })]
        })]
      })]
    }), _jsxs(View, {
      style: styles.section,
      children: [_jsx(Text, {
        style: styles.sectionTitle,
        children: "\u0421\u0442\u0430\u0442\u0443\u0441\u044B \u0442\u0440\u0435\u0432\u043E\u0433"
      }), _jsxs(View, {
        style: styles.overviewRow,
        children: [_jsxs(View, {
          style: styles.overviewCard,
          children: [_jsx(AlertCircle, {
            size: 28,
            color: "#ef4444"
          }), _jsx(Text, {
            style: styles.overviewValue,
            children: statistics.unresolvedCount
          }), _jsx(Text, {
            style: styles.overviewLabel,
            children: "\u0410\u043A\u0442\u0438\u0432\u043D\u044B\u0445 \u0442\u0440\u0435\u0432\u043E\u0433"
          })]
        }), _jsxs(View, {
          style: styles.overviewCard,
          children: [_jsx(TrendingUp, {
            size: 28,
            color: "#22c55e"
          }), _jsx(Text, {
            style: styles.overviewValue,
            children: statistics.resolvedCount
          }), _jsx(Text, {
            style: styles.overviewLabel,
            children: "\u0420\u0435\u0448\u0435\u043D\u043E"
          })]
        })]
      })]
    }), _jsxs(View, {
      style: styles.section,
      children: [_jsxs(View, {
        style: styles.sectionHeader,
        children: [_jsx(Text, {
          style: styles.sectionTitle,
          children: "\u0420\u0430\u0441\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u0438\u0435 \u0440\u0438\u0441\u043A\u043E\u0432"
        }), _jsx(RefreshCcw, {
          size: 18,
          color: theme.textSecondary
        })]
      }), _jsx(View, {
        style: styles.chartCard,
        children: Object.keys(statistics.riskDistribution).map(function (level) {
          var count = statistics.riskDistribution[level];
          var barWidth = maxRiskCount > 0 ? count / maxRiskCount * 100 : 0;
          var percentage = statistics.analyzedMessages > 0 ? count / statistics.analyzedMessages * 100 : 0;
          return _jsxs(View, {
            style: styles.barRow,
            children: [_jsx(Text, {
              style: styles.barLabel,
              children: RISK_LABELS[level]
            }), _jsx(View, {
              style: styles.barTrack,
              children: _jsx(View, {
                style: [styles.barFill, {
                  width: `${barWidth}%`,
                  backgroundColor: RISK_COLORS[level]
                }]
              })
            }), _jsxs(Text, {
              style: styles.barValue,
              children: [count, " \u2022 ", percentage.toFixed(0), "%"]
            })]
          }, level);
        })
      })]
    }), _jsxs(View, {
      style: styles.section,
      children: [_jsx(Text, {
        style: styles.sectionTitle,
        children: "\u0421\u043E\u0441\u0442\u043E\u044F\u043D\u0438\u0435 \u0447\u0430\u0442\u043E\u0432"
      }), _jsx(View, {
        style: styles.chatGrid,
        children: Object.keys(statistics.chatRiskDistribution).map(function (level) {
          return _jsxs(View, {
            style: [styles.chatCard, {
              borderLeftColor: RISK_COLORS[level]
            }],
            testID: `security-chat-${level}`,
            children: [_jsx(Text, {
              style: styles.chatValue,
              children: statistics.chatRiskDistribution[level]
            }), _jsx(Text, {
              style: styles.chatLabel,
              children: RISK_LABELS[level]
            })]
          }, level);
        })
      })]
    }), _jsxs(View, {
      style: styles.section,
      children: [_jsx(Text, {
        style: styles.sectionTitle,
        children: "\u041F\u043E\u043A\u0440\u044B\u0442\u0438\u0435 \u0437\u0430\u0449\u0438\u0442\u044B"
      }), _jsxs(View, {
        style: styles.progressCard,
        children: [_jsxs(View, {
          style: styles.progressRow,
          children: [_jsx(Text, {
            style: styles.progressLabel,
            children: "\u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u044F \u043F\u043E\u0434 \u043D\u0430\u0431\u043B\u044E\u0434\u0435\u043D\u0438\u0435\u043C"
          }), _jsxs(Text, {
            style: styles.progressValue,
            children: [statistics.totalMessages > 0 ? (statistics.analyzedMessages / statistics.totalMessages * 100).toFixed(1) : '0.0', "%"]
          })]
        }), _jsx(View, {
          style: styles.progressTrack,
          children: _jsx(View, {
            style: [styles.progressFill, {
              width: statistics.totalMessages > 0 ? `${statistics.analyzedMessages / statistics.totalMessages * 100}%` : '0%'
            }]
          })
        })]
      })]
    })]
  });
}
