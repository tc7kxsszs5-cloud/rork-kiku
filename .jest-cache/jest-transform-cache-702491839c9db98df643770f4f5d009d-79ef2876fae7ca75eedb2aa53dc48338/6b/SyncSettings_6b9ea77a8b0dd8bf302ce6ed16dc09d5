d4e202cc6741f3fd3620cc7e14b62481
import _asyncToGenerator from "@babel/runtime/helpers/asyncToGenerator";
import React from 'react';
import { View, Text, StyleSheet, Switch, TouchableOpacity, ActivityIndicator } from 'react-native';
import { RefreshCcw, Cloud, HardDrive, CheckCircle2, XCircle, Clock } from 'lucide-react-native';
import { useSyncSettings } from '@/constants/SyncSettingsContext';
import { useThemeMode } from '@/constants/ThemeContext';
import { HapticFeedback } from '@/constants/haptics';
import { jsx as _jsx, jsxs as _jsxs, Fragment as _Fragment } from "react/jsx-runtime";
var SYNC_FREQUENCY_OPTIONS = [{
  value: 'realtime',
  label: 'В реальном времени',
  description: 'Мгновенная синхронизация'
}, {
  value: '5min',
  label: 'Каждые 5 минут',
  description: 'Быстрая синхронизация'
}, {
  value: '15min',
  label: 'Каждые 15 минут',
  description: 'Умеренная синхронизация'
}, {
  value: '1hour',
  label: 'Каждый час',
  description: 'Экономная синхронизация'
}];
var SYNC_SOURCE_OPTIONS = [{
  value: 'cloud',
  label: 'Облако',
  description: 'Синхронизация через сервер',
  Icon: Cloud
}, {
  value: 'local',
  label: 'Локально',
  description: 'Только на устройстве',
  Icon: HardDrive
}];
export function SyncSettings() {
  var _useThemeMode = useThemeMode(),
    theme = _useThemeMode.theme;
  var _useSyncSettings = useSyncSettings(),
    settings = _useSyncSettings.settings,
    isLoading = _useSyncSettings.isLoading,
    isSyncing = _useSyncSettings.isSyncing,
    updateSettings = _useSyncSettings.updateSettings,
    triggerSync = _useSyncSettings.triggerSync;
  var handleToggleAutoSync = function () {
    var _ref = _asyncToGenerator(function* (value) {
      HapticFeedback.selection();
      yield updateSettings({
        autoSyncEnabled: value
      });
    });
    return function handleToggleAutoSync(_x) {
      return _ref.apply(this, arguments);
    };
  }();
  var handleFrequencyChange = function () {
    var _ref2 = _asyncToGenerator(function* (frequency) {
      HapticFeedback.selection();
      yield updateSettings({
        frequency: frequency
      });
    });
    return function handleFrequencyChange(_x2) {
      return _ref2.apply(this, arguments);
    };
  }();
  var handleSourceChange = function () {
    var _ref3 = _asyncToGenerator(function* (source) {
      HapticFeedback.selection();
      yield updateSettings({
        source: source
      });
    });
    return function handleSourceChange(_x3) {
      return _ref3.apply(this, arguments);
    };
  }();
  var handleManualSync = function () {
    var _ref4 = _asyncToGenerator(function* () {
      HapticFeedback.medium();
      yield triggerSync();
    });
    return function handleManualSync() {
      return _ref4.apply(this, arguments);
    };
  }();
  var formatLastSync = function formatLastSync() {
    if (!settings.lastSyncTimestamp) {
      return 'Синхронизация не выполнялась';
    }
    var date = new Date(settings.lastSyncTimestamp);
    var now = Date.now();
    var diff = now - settings.lastSyncTimestamp;
    var minutes = Math.floor(diff / 60000);
    if (minutes < 1) return 'Только что';
    if (minutes < 60) return `${minutes} мин. назад`;
    var hours = Math.floor(minutes / 60);
    if (hours < 24) return `${hours} ч. назад`;
    return date.toLocaleDateString('ru-RU', {
      day: '2-digit',
      month: '2-digit',
      hour: '2-digit',
      minute: '2-digit'
    });
  };
  var styles = createStyles(theme);
  if (isLoading) {
    return _jsx(View, {
      style: styles.container,
      children: _jsx(ActivityIndicator, {
        size: "small",
        color: theme.accentPrimary
      })
    });
  }
  return _jsxs(View, {
    style: styles.container,
    children: [_jsxs(View, {
      style: styles.settingRow,
      children: [_jsxs(View, {
        style: styles.settingInfo,
        children: [_jsx(Text, {
          style: styles.settingTitle,
          children: "\u0410\u0432\u0442\u043E\u0441\u0438\u043D\u0445\u0440\u043E\u043D\u0438\u0437\u0430\u0446\u0438\u044F"
        }), _jsx(Text, {
          style: styles.settingDescription,
          children: "\u0410\u0432\u0442\u043E\u043C\u0430\u0442\u0438\u0447\u0435\u0441\u043A\u0430\u044F \u0441\u0438\u043D\u0445\u0440\u043E\u043D\u0438\u0437\u0430\u0446\u0438\u044F \u0434\u0430\u043D\u043D\u044B\u0445"
        })]
      }), _jsx(Switch, {
        value: settings.autoSyncEnabled,
        onValueChange: handleToggleAutoSync,
        trackColor: {
          false: theme.borderSoft,
          true: theme.accentPrimary
        },
        thumbColor: theme.isDark ? '#fff' : '#fff'
      })]
    }), settings.autoSyncEnabled && _jsxs(View, {
      style: styles.section,
      children: [_jsx(Text, {
        style: styles.sectionTitle,
        children: "\u0427\u0430\u0441\u0442\u043E\u0442\u0430 \u0441\u0438\u043D\u0445\u0440\u043E\u043D\u0438\u0437\u0430\u0446\u0438\u0438"
      }), SYNC_FREQUENCY_OPTIONS.map(function (option) {
        return _jsxs(TouchableOpacity, {
          style: [styles.optionRow, settings.frequency === option.value && styles.optionRowActive],
          onPress: function onPress() {
            return handleFrequencyChange(option.value);
          },
          children: [_jsxs(View, {
            style: styles.optionInfo,
            children: [_jsx(Text, {
              style: [styles.optionLabel, settings.frequency === option.value && styles.optionLabelActive],
              children: option.label
            }), _jsx(Text, {
              style: styles.optionDescription,
              children: option.description
            })]
          }), settings.frequency === option.value && _jsx(CheckCircle2, {
            size: 20,
            color: theme.accentPrimary
          })]
        }, option.value);
      })]
    }), _jsxs(View, {
      style: styles.section,
      children: [_jsx(Text, {
        style: styles.sectionTitle,
        children: "\u0418\u0441\u0442\u043E\u0447\u043D\u0438\u043A \u0434\u0430\u043D\u043D\u044B\u0445"
      }), SYNC_SOURCE_OPTIONS.map(function (option) {
        var Icon = option.Icon;
        return _jsxs(TouchableOpacity, {
          style: [styles.optionRow, settings.source === option.value && styles.optionRowActive],
          onPress: function onPress() {
            return handleSourceChange(option.value);
          },
          children: [_jsx(View, {
            style: styles.optionIcon,
            children: _jsx(Icon, {
              size: 20,
              color: settings.source === option.value ? theme.accentPrimary : theme.textSecondary
            })
          }), _jsxs(View, {
            style: styles.optionInfo,
            children: [_jsx(Text, {
              style: [styles.optionLabel, settings.source === option.value && styles.optionLabelActive],
              children: option.label
            }), _jsx(Text, {
              style: styles.optionDescription,
              children: option.description
            })]
          }), settings.source === option.value && _jsx(CheckCircle2, {
            size: 20,
            color: theme.accentPrimary
          })]
        }, option.value);
      })]
    }), _jsxs(View, {
      style: styles.statusCard,
      children: [_jsxs(View, {
        style: styles.statusHeader,
        children: [_jsx(Clock, {
          size: 16,
          color: theme.textSecondary
        }), _jsx(Text, {
          style: styles.statusLabel,
          children: "\u041F\u043E\u0441\u043B\u0435\u0434\u043D\u044F\u044F \u0441\u0438\u043D\u0445\u0440\u043E\u043D\u0438\u0437\u0430\u0446\u0438\u044F"
        })]
      }), _jsx(Text, {
        style: styles.statusValue,
        children: formatLastSync()
      }), settings.lastSyncStatus && _jsx(View, {
        style: styles.statusIndicator,
        children: settings.lastSyncStatus === 'success' ? _jsxs(_Fragment, {
          children: [_jsx(CheckCircle2, {
            size: 14,
            color: "#22c55e"
          }), _jsx(Text, {
            style: styles.statusText,
            children: "\u0423\u0441\u043F\u0435\u0448\u043D\u043E"
          })]
        }) : settings.lastSyncStatus === 'error' ? _jsxs(_Fragment, {
          children: [_jsx(XCircle, {
            size: 14,
            color: "#ef4444"
          }), _jsx(Text, {
            style: styles.statusText,
            children: "\u041E\u0448\u0438\u0431\u043A\u0430"
          })]
        }) : _jsxs(_Fragment, {
          children: [_jsx(ActivityIndicator, {
            size: "small",
            color: theme.accentPrimary
          }), _jsx(Text, {
            style: styles.statusText,
            children: "\u0412 \u043F\u0440\u043E\u0446\u0435\u0441\u0441\u0435..."
          })]
        })
      })]
    }), _jsx(TouchableOpacity, {
      style: [styles.syncButton, isSyncing && styles.syncButtonDisabled],
      onPress: handleManualSync,
      disabled: isSyncing,
      children: isSyncing ? _jsx(ActivityIndicator, {
        size: "small",
        color: theme.isDark ? '#0b1220' : '#1a1a1a'
      }) : _jsxs(_Fragment, {
        children: [_jsx(RefreshCcw, {
          size: 18,
          color: theme.isDark ? '#0b1220' : '#1a1a1a'
        }), _jsx(Text, {
          style: styles.syncButtonText,
          children: "\u0421\u0438\u043D\u0445\u0440\u043E\u043D\u0438\u0437\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u0441\u0435\u0439\u0447\u0430\u0441"
        })]
      })
    })]
  });
}
var createStyles = function createStyles(theme) {
  return StyleSheet.create({
    container: {
      gap: 20
    },
    settingRow: {
      flexDirection: 'row',
      alignItems: 'center',
      justifyContent: 'space-between',
      paddingVertical: 12,
      paddingHorizontal: 16,
      backgroundColor: theme.card,
      borderRadius: 16,
      borderWidth: 1,
      borderColor: theme.borderSoft
    },
    settingInfo: {
      flex: 1,
      marginRight: 16
    },
    settingTitle: {
      fontSize: 16,
      fontWeight: '600',
      color: theme.textPrimary,
      marginBottom: 4
    },
    settingDescription: {
      fontSize: 13,
      color: theme.textSecondary
    },
    section: {
      gap: 8
    },
    sectionTitle: {
      fontSize: 14,
      fontWeight: '600',
      color: theme.textSecondary,
      textTransform: 'uppercase',
      letterSpacing: 0.5,
      marginBottom: 4
    },
    optionRow: {
      flexDirection: 'row',
      alignItems: 'center',
      padding: 14,
      backgroundColor: theme.card,
      borderRadius: 12,
      borderWidth: 1,
      borderColor: theme.borderSoft
    },
    optionRowActive: {
      borderColor: theme.accentPrimary,
      backgroundColor: theme.isDark ? `rgba(${parseInt(theme.accentPrimary.slice(1, 3), 16)}, ${parseInt(theme.accentPrimary.slice(3, 5), 16)}, ${parseInt(theme.accentPrimary.slice(5, 7), 16)}, 0.15)` : `${theme.accentPrimary}15`
    },
    optionIcon: {
      marginRight: 12
    },
    optionInfo: {
      flex: 1
    },
    optionLabel: {
      fontSize: 15,
      fontWeight: '600',
      color: theme.textPrimary,
      marginBottom: 2
    },
    optionLabelActive: {
      color: theme.accentPrimary
    },
    optionDescription: {
      fontSize: 13,
      color: theme.textSecondary
    },
    statusCard: {
      padding: 16,
      backgroundColor: theme.cardMuted,
      borderRadius: 16,
      borderWidth: 1,
      borderColor: theme.borderSoft,
      gap: 8
    },
    statusHeader: {
      flexDirection: 'row',
      alignItems: 'center',
      gap: 8
    },
    statusLabel: {
      fontSize: 13,
      fontWeight: '600',
      color: theme.textSecondary,
      textTransform: 'uppercase',
      letterSpacing: 0.5
    },
    statusValue: {
      fontSize: 16,
      fontWeight: '600',
      color: theme.textPrimary
    },
    statusIndicator: {
      flexDirection: 'row',
      alignItems: 'center',
      gap: 6,
      marginTop: 4
    },
    statusText: {
      fontSize: 13,
      color: theme.textSecondary
    },
    syncButton: {
      flexDirection: 'row',
      alignItems: 'center',
      justifyContent: 'center',
      gap: 10,
      backgroundColor: theme.accentPrimary,
      borderRadius: 16,
      paddingVertical: 14
    },
    syncButtonDisabled: {
      opacity: 0.6
    },
    syncButtonText: {
      fontSize: 16,
      fontWeight: '700',
      color: theme.isDark ? '#0b1220' : '#1a1a1a'
    }
  });
};
