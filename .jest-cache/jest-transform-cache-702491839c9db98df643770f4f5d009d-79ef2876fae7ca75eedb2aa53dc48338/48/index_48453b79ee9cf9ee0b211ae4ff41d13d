    var GROUP_TYPE_EMOJI = {
      class: 'ðŸ«',
      club: 'âš½',
      group: 'ðŸ“š'
    };
    var groupEmoji = item.isGroup && item.groupType ? GROUP_TYPE_EMOJI[item.groupType] : 'ðŸ’¬';
    return _jsxs(TouchableOpacity, {
      style: styles.chatCard,
      onPress: function onPress() {
        HapticFeedback.light();
        router.push(`/chat/${item.id}`);
      },
      children: [_jsxs(View, {
        style: styles.chatHeader,
        children: [_jsx(View, {
