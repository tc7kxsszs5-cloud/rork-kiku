            children: groupEmoji
          })
        }), _jsxs(View, {
          style: styles.chatInfo,
          children: [_jsxs(View, {
            style: styles.chatTitleRow,
            children: [_jsx(Text, {
              style: styles.chatTitle,
              children: chatTitle
            }), !item.isGroup && _jsx(OnlineStatus, {
              size: "small",
              style: styles.chatStatus
            })]
          }), _jsx(Text, {
            style: styles.chatSubtitle,
            children: chatSubtitle
          }), _jsxs(Text, {
            style: styles.lastActivity,
            children: ["\u041F\u043E\u0441\u043B\u0435\u0434\u043D\u044F\u044F \u0430\u043A\u0442\u0438\u0432\u043D\u043E\u0441\u0442\u044C: ", lastActivityText]
          })]
        })]
      }), _jsx(View, {
        style: styles.chatFooter,
        children: item.overallRisk && _jsxs(View, {
          style: [styles.riskBadge, {
            backgroundColor: RISK_COLORS[item.overallRisk] || RISK_COLORS.safe
          }],
          children: [_jsx(Shield, {
            size: 14,
            color: "#fff"
          }), _jsx(Text, {
            style: styles.riskText,
            children: RISK_LABELS[item.overallRisk] || RISK_LABELS.safe
          })]
        })
      })]
    });
  }, [router]);
