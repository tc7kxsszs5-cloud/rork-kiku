  var styles = useMemo(function () {
    return createStyles(theme);
  }, [theme]);
  var totalChats = (chats == null ? void 0 : chats.length) || 0;
  var totalMessages = (chats == null ? void 0 : chats.reduce(function (sum, chat) {
    var _chat$messages;
    return sum + (((_chat$messages = chat.messages) == null ? void 0 : _chat$messages.length) || 0);
  }, 0)) || 0;
  var allParticipants = useMemo(function () {
    if (!chats || !Array.isArray(chats)) return [];
    var participantsSet = new Set();
    chats.forEach(function (chat) {
      if (chat.participants) {
        chat.participants.forEach(function (p) {
          return participantsSet.add(p);
        });
      }
      if (chat.participantNames) {
        chat.participantNames.forEach(function (n) {
          return participantsSet.add(n);
        });
      }
    });
    return Array.from(participantsSet).sort();
  }, [chats]);
  var filteredChats = useMemo(function () {
    if (!chats || !Array.isArray(chats)) return [];
    var result = chats;
    if (searchQuery.trim()) {
      var query = searchQuery.toLowerCase();
      result = result.filter(function (chat) {
        var nameMatch = chat.isGroup && chat.groupName ? chat.groupName.toLowerCase().includes(query) : chat.participantNames.some(function (name) {
          return name.toLowerCase().includes(query);
        });
        var messageMatch = chat.messages.some(function (msg) {
          return msg.text.toLowerCase().includes(query);
        });
        return nameMatch || messageMatch;
      });
    }
    if (selectedRiskFilter !== 'all') {
      result = result.filter(function (chat) {
        return chat.overallRisk === selectedRiskFilter;
      });
    }
    if (dateFilter !== 'all') {
      var now = Date.now();
      var dayMs = 24 * 60 * 60 * 1000;
      var timeThreshold;
      switch (dateFilter) {
        case 'today':
          timeThreshold = now - dayMs;
          break;
        case 'week':
          timeThreshold = now - 7 * dayMs;
          break;
        case 'month':
          timeThreshold = now - 30 * dayMs;
          break;
        default:
          timeThreshold = 0;
      }
      result = result.filter(function (chat) {
        return chat.lastActivity >= timeThreshold;
      });
    }
    if (participantFilter !== 'all') {
      result = result.filter(function (chat) {
        return chat.participants.includes(participantFilter) || chat.participantNames.includes(participantFilter);
      });
    }
    return result;
  }, [chats, searchQuery, selectedRiskFilter, dateFilter, participantFilter]);
  var toggleSearch = function toggleSearch() {
    HapticFeedback.light();
    setIsSearchVisible(!isSearchVisible);
    Animated.timing(searchBarHeight, {
      toValue: isSearchVisible ? 0 : 60,
      duration: 250,
      useNativeDriver: false
    }).start();
  };
  var handleRiskFilter = function handleRiskFilter(risk) {
    HapticFeedback.selection();
    setSelectedRiskFilter(risk);
  };
  var renderChat = useCallback(function (_ref) {
    var _item$messages, _item$participantName, _item$participants;
    var item = _ref.item;
    if (!item) return null;
    var messageCount = ((_item$messages = item.messages) == null ? void 0 : _item$messages.length) || 0;
    var lastActivity = item.lastActivity || Date.now();
    var lastActivityText = new Date(lastActivity).toLocaleString('ru-RU', {
      day: '2-digit',
      month: '2-digit',
      hour: '2-digit',
      minute: '2-digit'
    });
    var chatTitle = item.isGroup ? item.groupName || 'Ð“Ñ€ÑƒÐ¿Ð¿Ð°' : ((_item$participantName = item.participantNames) == null ? void 0 : _item$participantName.join(' Ð¸ ')) || 'Ð§Ð°Ñ‚';
