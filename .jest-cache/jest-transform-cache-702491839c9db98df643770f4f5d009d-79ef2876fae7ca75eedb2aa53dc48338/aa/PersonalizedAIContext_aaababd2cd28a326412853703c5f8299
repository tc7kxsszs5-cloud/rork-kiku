    var personalizeAnalysis = useCallback(function (childId, analysis, messageText) {
      var profile = profiles.get(childId);
      if (!profile) {
        return analysis;
      }
      var normalizedText = messageText.toLowerCase();
      var isWhitelisted = profile.personalizedRules.whitelistedPhrases.some(function (phrase) {
        return normalizedText.includes(phrase.toLowerCase());
      });
      if (isWhitelisted && analysis.riskLevel !== 'critical') {
        var levels = ['safe', 'low', 'medium', 'high', 'critical'];
        var currentIndex = levels.indexOf(analysis.riskLevel);
        if (currentIndex > 0) {
          return Object.assign({}, analysis, {
            riskLevel: levels[currentIndex - 1],
            confidence: analysis.confidence * 0.7,
            reasons: [].concat(_toConsumableArray(analysis.reasons), ['Персонализированная проверка: фраза в whitelist'])
          });
        }
      }
      var sensitivity = profile.personalizedRules.sensitivityLevel;
      var falsePositiveRate = profile.learningData.falsePositives / (profile.learningData.falsePositives + profile.learningData.truePositives || 1);
      if (falsePositiveRate > 0.3 && analysis.riskLevel !== 'critical') {
        var adjustedConfidence = analysis.confidence * (1 - falsePositiveRate);
        if (adjustedConfidence < 0.5) {
          return Object.assign({}, analysis, {
            riskLevel: 'safe',
            confidence: adjustedConfidence,
            reasons: [].concat(_toConsumableArray(analysis.reasons), ['Скорректировано на основе истории ложных срабатываний'])
          });
        }
      }
      if (profile.age < 10 && analysis.riskLevel === 'low') {
        return Object.assign({}, analysis, {
          riskLevel: 'medium',
          confidence: analysis.confidence * 1.2,
          reasons: [].concat(_toConsumableArray(analysis.reasons), ['Усилено для младшего возраста'])
        });
      }
      return analysis;
