          messageId: messageId,
          wasCorrect: wasCorrect,
          timestamp: Date.now()
        };
        yield updateChildProfile(childId, {
          learningData: Object.assign({}, profile.learningData, {
            falsePositives: wasCorrect ? profile.learningData.falsePositives : profile.learningData.falsePositives + 1,
            truePositives: wasCorrect ? profile.learningData.truePositives + 1 : profile.learningData.truePositives,
            parentFeedback: [].concat(_toConsumableArray(profile.learningData.parentFeedback), [feedback]).slice(-100)
          })
