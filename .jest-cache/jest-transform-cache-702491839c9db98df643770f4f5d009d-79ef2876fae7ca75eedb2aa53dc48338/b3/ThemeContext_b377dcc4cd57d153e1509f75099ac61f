      cov_nn985clxe().f[9]++;
      cov_nn985clxe().s[33]++;
      setThemeMode(themeMode === 'sunrise' ? (cov_nn985clxe().b[6][0]++, 'midnight') : (cov_nn985clxe().b[6][1]++, 'sunrise'));
    }, [setThemeMode, themeMode]));
