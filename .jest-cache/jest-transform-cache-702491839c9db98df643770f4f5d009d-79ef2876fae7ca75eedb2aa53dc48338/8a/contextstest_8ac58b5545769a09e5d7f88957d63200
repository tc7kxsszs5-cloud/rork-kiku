import _slicedToArray from "@babel/runtime/helpers/slicedToArray";
import _asyncToGenerator from "@babel/runtime/helpers/asyncToGenerator";
import React from 'react';
import { render, waitFor } from '@testing-library/react-native';
import { QueryClient, QueryClientProvider } from '@tanstack/react-query';
import { jsxs as _jsxs, jsx as _jsx } from "react/jsx-runtime";
describe('Контексты', function () {
  var queryClient;
  beforeEach(function () {
    queryClient = new QueryClient({
      defaultOptions: {
        queries: {
          retry: false
        },
        mutations: {
          retry: false
        }
      }
    });
  });
  var testContext = function testContext(contextName, hookName) {
    test(`${contextName} инициализируется без ошибок`, _asyncToGenerator(function* () {
      try {
        var module = yield import(`@/constants/${contextName}`);
        var Provider;
        var useHook;
        if (Array.isArray(module.default)) {
          var _module$default = _slicedToArray(module.default, 2);
          Provider = _module$default[0];
          useHook = _module$default[1];
        } else {
          var _module$default2, _module$default3;
          var providerName = `${contextName.replace('Context', '')}Provider`;
          Provider = module[providerName] || module.Provider || ((_module$default2 = module.default) == null ? void 0 : _module$default2.Provider);
          useHook = module[hookName] || module.useHook || ((_module$default3 = module.default) == null ? void 0 : _module$default3[hookName]);
        }
        if (!Provider || !useHook) {
          console.warn(`⚠️  ${contextName} не экспортирует Provider или hook`);
          console.warn(`   Доступные ключи:`, Object.keys(module));
          return;
        }
        var TestComponent = function TestComponent() {
          try {
            var hook = useHook();
            return _jsxs("div", {
              children: ["Test - ", hook ? 'Hook loaded' : 'Hook empty']
            });
          } catch (hookError) {
            console.error(`Hook error in ${contextName}:`, hookError);
            return _jsx("div", {
              children: "Hook Error"
            });
          }
        };
        var _render = render(_jsx(QueryClientProvider, {
            client: queryClient,
            children: _jsx(Provider, {
              children: _jsx(TestComponent, {})
            })
          })),
          container = _render.container;
        yield waitFor(function () {
          expect(container).toBeTruthy();
        }, {
          timeout: 5000
        });
      } catch (error) {
        var errorMessage = error instanceof Error ? error.message : String(error);
        var errorStack = error instanceof Error ? error.stack : undefined;
        console.error(`Error testing ${contextName}:`, errorMessage, errorStack);
        throw new Error(`${contextName} не работает: ${errorMessage}`);
      }
    }));
  };
  testContext('ThemeContext', 'useThemeMode');
  testContext('UserContext', 'useUser');
});
