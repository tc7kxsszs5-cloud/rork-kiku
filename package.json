{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "node scripts/patch-jest-runtime.js",
    "start": "./scripts/smart-start.sh",
    "start:rork": "./scripts/smart-start.sh",
    "start:rork:tunnel": "./scripts/start-tunnel-smart.sh",
    "start:rork:tunnel:direct": "echo '⚠️  Tunnel режим нестабилен. Используйте bun run start (LAN) для надежной работы.' && bunx rork start -p d8v7u672uumlfpscvnbps --tunnel --scheme rork-app",
    "start:rork:tunnel:timeout": "./scripts/start-tunnel-with-timeout.sh",
    "start:rork:lan": "bunx rork start -p d8v7u672uumlfpscvnbps --lan --scheme rork-app",
    "start:expo": "bunx expo start",
    "start:local": "EXPO_USE_DEV_CLIENT=false bunx expo start --lan",
    "start:simple": "EXPO_NO_DOTENV=1 bunx expo start --lan --no-dev",
    "start:clear": "bunx expo start --lan --clear",
    "start:web": "bunx expo start --web --port 8082",
    "start-web": "bunx expo start --web",
    "start-web-dev": "DEBUG=expo* bunx rork start -p d8v7u672uumlfpscvnbps --web --tunnel",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "typecheck": "bunx tsc --noEmit",
    "check": "./scripts/auto-check-and-fix.sh",
    "check:fix": "./scripts/auto-check-and-fix.sh --fix",
    "prelaunch": "./scripts/pre-launch-check.sh",
    "test": "bunx jest",
    "test:watch": "bunx jest --watch",
    "test:coverage": "bunx jest --coverage",
    "test:unit": "bunx jest --testPathPattern=unit",
    "test:integration": "bunx jest --testPathPattern=integration --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:report": "playwright show-report",
    "test:playwright:fix": "./scripts/fix-playwright-port.sh",
    "test:bun": "bun test __tests__/unit",
    "test:jest": "bunx jest --testPathPattern=\"integration|e2e|soundNotifications\" --maxWorkers=2",
    "test:all": "bun test __tests__/unit && bunx jest --testPathPattern=\"integration|e2e|soundNotifications\" --maxWorkers=2",
    "test:startup": "bun run scripts/test-app-startup.ts",
    "test:deep": "bun run scripts/deep-config-test.ts",
    "test:config": "bun run test:startup && bun run test:deep",
    "test:all-config": "./scripts/test-all-config.sh",
    "fix": "./scripts/quick-fix.sh",
    "fix:startup": "./scripts/fix-startup-issues.sh",
    "kill-ports": "./scripts/kill-ports.sh",
    "diagnose": "bun run scripts/comprehensive-diagnosis.ts",
    "ci:install": "bun install",
    "ci:lint": "bun run lint",
    "ci:tsc": "bunx tsc --noEmit",
    "ci:test": "jest --ci --coverage --maxWorkers=2",
    "ci:all": "bun run ci:install && bun run ci:lint && bun run ci:tsc && bun run ci:test",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:sim": "./scripts/ios-sim-auto.sh",
    "ios:sim:auto": "./scripts/ios-sim-auto.sh",
    "ios:sim:safe": "./scripts/ios-sim-safe.sh",
    "ios:sim:fix": "./scripts/fix-simulator.sh",
    "ios:sim:boot": "./scripts/boot-simulator.sh",
    "ios:xcode:check": "./scripts/fix-xcode-location.sh",
    "ios:debug": "./scripts/ios-sim-debug.sh",
    "ios:setup": "./scripts/setup-ios.sh",
    "ios:cocoapods": "./scripts/install-cocoapods.sh",
    "android:emulator": "bunx expo start --android"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hono/trpc-server": "^0.4.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@rork-ai/toolkit-sdk": "*",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.90.11",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@ungap/structured-clone": "^1.3.0",
    "expo": "~54.0.31",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-contacts": "~15.0.11",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "^17.0.8",
    "expo-localization": "^17.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "hono": "^4.10.6",
    "i18next": "^25.6.3",
    "lucide-react-native": "^0.475.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.3.5",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "superjson": "^2.2.5",
    "zod": "^4.1.13",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^54.0.22",
    "@expo/ngrok": "^4.1.0",
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "playwright": "^1.58.0",
    "react-test-renderer": "19.1.0",
    "typescript": "5.9.2"
  },
  "overrides": {
    "stack-utils": "2.0.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true
}